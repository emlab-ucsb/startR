dir2tree <- function(dir, depth=NULL, vertical=FALSE){
if(!dir.exists(dir)){
stop(sprintf("Folder %s not found.", dir))
}
.dir2tree(dir, ifelse(is.null(depth) || is.na(depth) || is.infinite(depth), -1L, depth), as.integer(vertical))
}
